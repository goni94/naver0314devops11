package day0319;

public class Ex1_Munje {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int count = 0;
		
		Loop:
		for (int i=1; i<=100; i++) {
			for (int j=2; j<i; j++) {
				
				if (i%j==0)
					continue Loop; // i++ 로 이동
				
			}
			count++;
			System.out.printf("%d\n",i);
			
		}
		System.out.println("\n소수의 갯수 :" +count);
	}

}
